{
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "../",
    "moduleNameMapper":{
      "^@helpers$":"<rootDir>/src/helpers",
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "testRegex": ".*\\.e2e-spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }